<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
<link rel="stylesheet" href="/static/css/style.css">
    <title>Andriod</title>
  </head>
  <body>
    {{>navbar data=details}}

    {{#each andriods}}
    <div class="andriod-section">
      <div class="container">
        <h1 class="mt-5">{{this.header1}}
</h1>
        <p class="mt-3">{{this.para1}}</p>
     <h3 class="mt-5">{{this.header2}}</h3>
     <ol class="mt-4">
      <li><p>{{this.list1}}</p></li>
      <li><p>{{this.list2}}</p></li>
      <li><p>{{this.list3}}</p></li>
      <li><p>{{this.list4}}</p></li>
      <li><p>{{this.list5}}</p></li>
      <li><p>{{this.list6}}</p></li>
      <li><p>{{this.list7}}</p></li>
      <li><p>{{this.list8}}</p></li>
      <li><p>{{this.list9}}</p></li>
      <li><p>{{this.list10}}</p></li>

     </ol>

     <div class="background-andriod mt-5">
      <p>{{this.background1para1}}</p>
      <p>c{{this.background1para2}}</p>
      <p>{{this.background1para3}}</p>
      


     </div>

     <div class="background-andriod1 mt-5">
      <p>{{this.background2.para}}</p>
     </div>


     <h3 class="mt-5">{{this.header3}}</h3>
     <p>{{this.subtitle}}</p>
     <img src="{{this.img}}" alt="system-interaction" width="100%">
      </div>
    </div>
{{/each}}




<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
  </body>
</html>